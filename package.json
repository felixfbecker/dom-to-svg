{
	"name": "dom-to-svg",
	"version": "0.1.2",
	"description": "Take SVG screenshots of DOM elements",
	"main": "lib/index.js",
	"sideEffects": false,
	"files": [
		"lib"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/felixfbecker/dom-to-svg"
	},
	"browserslist": [
		"last 2 Chrome versions",
		"last 2 Firefox versions"
	],
	"keywords": [
		"svg",
		"dom",
		"screenshot",
		"snapshot",
		"document",
		"element",
		"image"
	],
	"scripts": {
		"build": "tsc -p .",
		"watch": "tsc -p . -w",
		"eslint": "eslint 'src/**/*.ts'",
		"prettier": "prettier --check '**/*.{yml,ts,json}'",
		"get-fixture": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node src/test/get-fixture.ts",
		"test": "mocha --require source-map-support/register lib/test/test.js",
		"semantic-release": "semantic-release"
	},
	"mocha": {
		"timeout": 150000,
		"exit": true,
		"enableSourceMaps": true,
		"watchFiles": [
			"lib/**/*.js"
		]
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"release": {
		"branches": [
			"main"
		]
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"author": "Felix Becker",
	"license": "MIT",
	"devDependencies": {
		"@commitlint/cli": "^11.0.0",
		"@commitlint/config-conventional": "^11.0.0",
		"@pollyjs/adapter-puppeteer": "^5.0.0",
		"@pollyjs/core": "^5.0.0",
		"@pollyjs/persister-fs": "^5.0.0",
		"@sourcegraph/eslint-config": "^0.20.16",
		"@sourcegraph/prettierrc": "^3.0.3",
		"@types/chai": "^4.2.14",
		"@types/content-type": "^1.1.3",
		"@types/lodash": "^4.14.165",
		"@types/mime-types": "^2.1.0",
		"@types/mocha": "^8.2.0",
		"@types/node": "^14.14.14",
		"@types/parcel-bundler": "^1.12.1",
		"@types/pixelmatch": "^5.2.2",
		"@types/pngjs": "^3.4.2",
		"@types/pollyjs__adapter-puppeteer": "^4.0.1",
		"@types/pollyjs__core": "^4.3.1",
		"@types/pollyjs__persister-fs": "^2.0.1",
		"@types/prettier": "^2.1.5",
		"@types/puppeteer": "^5.4.2",
		"@types/type-is": "^1.6.3",
		"chai": "^4.2.0",
		"chardet": "^1.3.0",
		"content-type": "^1.0.4",
		"delay": "^4.4.0",
		"eslint": "^7.14.0",
		"husky": "^4.3.0",
		"iconv-lite": "^0.6.2",
		"lodash": "^4.17.20",
		"mime-types": "^2.1.27",
		"mocha": "^8.2.1",
		"parcel-bundler": "^1.12.4",
		"pixelmatch": "^5.2.1",
		"pngjs": "^6.0.0",
		"prettier": "^2.2.1",
		"puppeteer": "^5.5.0",
		"rxjs": "^6.6.3",
		"semantic-release": "^17.2.4",
		"source-map-support": "^0.5.19",
		"tagged-template-noop": "^2.1.1",
		"ts-node": "^9.0.0",
		"typescript": "^4.1.3",
		"xml-formatter": "^2.4.0"
	},
	"dependencies": {
		"gradient-parser": "^0.1.5",
		"postcss-value-parser": "^4.1.0"
	}
}
