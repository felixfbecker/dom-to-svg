{
	"extends": "@sourcegraph/eslint-config",
	"parserOptions": {
		"project": "tsconfig.json"
	},
	"rules": {
		"import/extensions": ["error", "ignorePackages"],
		"no-restricted-globals": ["error", "document", "window", "location"],
		"etc/throw-error": "off",
		"unicorn/import-index": "off",
		"callback-return": "off"
	}
}
